<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.operacionesservicioweb"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="19" />
    
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CreateActivity"
            android:label="@string/title_create_activity" >
        </activity>
        <activity
            android:name=".ReadActivity"
            android:label="@string/title_read_activity" >
        </activity>
        <activity
            android:name=".UpdateActivity"
            android:label="@string/title_update_activity" >
        </activity>
        <activity
            android:name=".DeleteActivity"
            android:label="@string/title_delete_activity" >
        </activity>
    </application>

</manifest>
